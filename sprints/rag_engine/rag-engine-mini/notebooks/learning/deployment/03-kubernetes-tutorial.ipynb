   "source": [
    "# Debugging Commands\n",
    "print(\"=== Kubernetes Debugging Commands ===\\n\")\n",
    "\n",
    "print(\"1. Check pod status:\")\n",
    "print(\"   kubectl get pods -o wide\")\n",
    "print(\"\\n2. Check pod events (for errors):\")\n",
    "print(\"   kubectl describe pod <pod-name>\")\n",
    "print(\"\\n3. Check pod logs:\")\n",
    "print(\"   kubectl logs <pod-name>\")\n",
    "print(\"\\n4. Check previous container logs (if crashed):\")\n",
    "print(\"   kubectl logs <pod-name> --previous\")\n",
    "print(\"\\n5. Execute commands in pod:\")\n",
    "print(\"   kubectl exec -it <pod-name> -- /bin/sh\")\n",
    "print(\"\\n6. Port forward for local testing:\")\n",
    "print(\"   kubectl port-forward <pod-name> 8080:80\")\n",
    "print(\"\\n7. Check resource usage:\")\n",
    "print(\"   kubectl top pods\")\n",
    "print(\"\\n8. Check events across all resources:\")\n",
    "print(\"   kubectl get events --sort-by='.lastTimestamp'\")"
   ]
