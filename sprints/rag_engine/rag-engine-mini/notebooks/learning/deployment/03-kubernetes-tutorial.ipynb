   "outputs": [],
   "source": [
    "# Check for common issues\n",
    "print(\"=== Checking for Common Issues ===\\n\")\n",
    "\n",
    "# 1. Check pods in CrashLoopBackOff\n",
    "print(\"1. Pods in CrashLoopBackOff:\")\n",
    "result = subprocess.run(\n",
    "    ['kubectl', 'get', 'pods', '--all-namespaces', '-o', 'json'],\n",
    "    capture_output=True,\n",
    "    text=True\n",
    ")\n",
    "pods = json.loads(result.stdout)\n",
    "crash_pods = [p for p in pods['items'] if 'CrashLoopBackOff' in str(p)]\n",
    "print(f\"   Found {len(crash_pods)} pods in CrashLoopBackOff\")\n",
    "\n",
    "# 2. Check pods with high restart counts\n",
    "print(\"\\n2. Pods with high restart counts:\")\n",
    "for pod in pods['items']:\n",
    "    restarts = pod['status'].get('containerStatuses', [{}])[0].get('restartCount', 0)\n",
    "    if restarts > 5:\n",
    "        print(f\"   {pod['metadata']['name']}: {restarts} restarts\")\n",
    "\n",
    "# 3. Check for ImagePullBackOff\n",
    "print(\"\\n3. Pods with ImagePullBackOff:\")\n",
    "image_pull_pods = [p for p in pods['items'] if 'ImagePullBackOff' in str(p)]\n",
    "print(f\"   Found {len(image_pull_pods)} pods with image pull issues\")\n",
    "\n",
    "# 4. Check pending pods\n",
    "print(\"\\n4. Pending pods (resource/scheduling issues):\")\n",
    "pending_pods = [p for p in pods['items'] if p['status'].get('phase') == 'Pending']\n",
    "print(f\"   Found {len(pending_pods)} pending pods\")\n",
    "\n",
    "if not any([crash_pods, image_pull_pods, pending_pods]):\n",
    "    print(\"\\n✅ No major issues detected!\")"
