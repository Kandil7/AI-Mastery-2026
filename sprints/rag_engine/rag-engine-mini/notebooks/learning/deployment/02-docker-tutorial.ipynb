    "## Part 2: Explore the Dockerfile"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Cell 4: Read and analyze Dockerfile\n",
    "import os\n",
    "\n",
    "dockerfile_path = '../../Dockerfile'\n",
    "\n",
    "if os.path.exists(dockerfile_path):\n",
    "    with open(dockerfile_path, 'r') as f:\n",
    "        dockerfile_content = f.read()\n",
    "    \n",
    "    print(\"üìÑ Dockerfile Analysis:\")\n",
    "    print(\"=\"*60)\n",
    "    \n",
    "    lines = dockerfile_content.split('\\n')\n",
    "    \n",
    "    # Count layers\n",
    "    commands = ['FROM', 'RUN', 'COPY', 'WORKDIR', 'EXPOSE', 'CMD', 'ENV']\n",
    "    layer_count = 0\n",
    "    \n",
    "    for line in lines:\n",
    "        stripped = line.strip()\n",
    "        if stripped and not stripped.startswith('#'):\n",
    "            for cmd in commands:\n",
    "                if stripped.startswith(cmd):\n",
    "                    layer_count += 1\n",
    "                    break\n",
    "    \n",
    "    print(f\"‚úÖ Total layers: {layer_count}\")\n",
    "    print(f\"‚úÖ File size: {len(dockerfile_content)} characters\")\n",
    "    \n",
    "    # Identify multi-stage\n",
    "    if 'as builder' in dockerfile_content.lower():\n",
    "        print(f\"‚úÖ Multi-stage build detected (optimized for size)\")\n",
    "    else:\n",
    "        print(f\"‚ö†Ô∏è  Single-stage build (consider multi-stage for optimization)\")\n",
    "    \n",
    "    # Check security features\n",
    "    security_features = []\n",
    "    if 'USER' in dockerfile_content:\n",
    "        security_features.append(\"Non-root user\")\n",
    "    if 'HEALTHCHECK' in dockerfile_content:\n",
    "        security_features.append(\"Health checks\")\n",
    "    if '.dockerignore' in dockerfile_content or os.path.exists('../../.dockerignore'):\n",
    "        security_features.append(\".dockerignore\")\n",
    "    \n",
    "    if security_features:\n",
    "        print(f\"\\n‚úÖ Security features found:\")\n",
    "        for feature in security_features:\n",
    "            print(f\"   ‚Ä¢ {feature}\")\n",
    "    else:\n",
    "        print(f\"\\n‚ö†Ô∏è  No security features detected\")\n",
    "        \n",
    "else:\n",
    "    print(f\"‚ùå Dockerfile not found at {dockerfile_path}\")\n",
    "    print(\"Make sure you're in the correct directory\")"
   ]
