    "## Part 5: Docker Compose Orchestration"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Cell 12: Analyze docker-compose.yml\n",
    "import yaml\n",
    "\n",
    "compose_path = 'docker-compose.yml'\n",
    "\n",
    "if os.path.exists(compose_path):\n",
    "    with open(compose_path, 'r') as f:\n",
    "        compose_content = f.read()\n",
    "    \n",
    "    # Parse YAML\n",
    "    compose = yaml.safe_load(compose_content)\n",
    "    \n",
    "    print(\"üìä Docker Compose Analysis:\")\n",
    "    print(\"=\"*60)\n",
    "    \n",
    "    # Version\n",
    "    version = compose.get('version', 'Not specified')\n",
    "    print(f\"Version: {version}\")\n",
    "    \n",
    "    # Services\n",
    "    services = compose.get('services', {})\n",
    "    print(f\"\\nServices ({len(services)} total):\")\n",
    "    \n",
    "    for svc_name, svc_config in services.items():\n",
    "        image = svc_config.get('image', 'Built from Dockerfile')\n",
    "        ports = svc_config.get('ports', [])\n",
    "        \n",
    "        print(f\"\\n  üì¶ {svc_name}:\")\n",
    "        print(f\"     Image: {image}\")\n",
    "        if ports:\n",
    "            print(f\"     Ports: {', '.join(str(p) for p in ports)}\")\n",
    "        \n",
    "        # Dependencies\n",
    "        depends_on = svc_config.get('depends_on', [])\n",
    "        if depends_on:\n",
    "            print(f\"     Depends on: {', '.join(depends_on)}\")\n",
    "    \n",
    "    # Volumes\n",
    "    volumes = compose.get('volumes', {})\n",
    "    if volumes:\n",
    "        print(f\"\\nüíæ Volumes ({len(volumes)}):\")\n",
    "        for vol in volumes.keys():\n",
    "            print(f\"   ‚Ä¢ {vol}\")\n",
    "    \n",
    "    # Networks\n",
    "    networks = compose.get('networks', {})\n",
    "    if networks:\n",
    "        print(f\"\\nüåê Networks ({len(networks)}):\")\n",
    "        for net in networks.keys():\n",
    "            print(f\"   ‚Ä¢ {net}\")\n",
    "    else:\n",
    "        print(f\"\\nüåê Using default network\")\n",
    "        \n",
    "else:\n",
    "    print(f\"‚ùå docker-compose.yml not found\")"
   ]
